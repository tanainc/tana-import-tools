{
  "name": "tana-import-tools",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts src/",
    "prepack": "rm -rf dist/ && ./node_modules/.bin/tsc  --project tsconfig.build.json",
    "build": "npm run prepack",
    "convert:roam": "npm run build && node dist/runner.js roam",
    "convert:workflowy": "npm run build && node dist/runner.js workflowy",
    "convert:logseq": "npm run build && node dist/runner.js logseq",
    "test": "vitest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@11",
  "volta": {
    "node": "22.0.0"
  }
}
